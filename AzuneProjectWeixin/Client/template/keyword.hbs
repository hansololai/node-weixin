<header>
    <button class="button-back">Back</button>
    <h2 class="title">Keyword Reply</h2>
</header>
<section class="content">
    <table class="messages">
        <tr><th>Keyword</th><th>Regular Reply Type</th><th>Regular Reply Title</th><th>Premium Reply Type</th><th>Premium Reply Title</th></tr>
        {{#each keywords}}
        <tr id="{{idKeywordReply}}"><td>{{Keyword}}</td><td>{{#if RegularReply.MsgType}}{{RegularReply.MsgType}}{{/if}}</td><td>{{#if RegularReply.Title}}{{RegularReply.Title}}{{/if}}</td><td>{{#if MemberReply.MsgType}}{{MemberReply.MsgType}}{{else}}NONE{{/if}}</td><td>{{#if MemberReply.Title}}{{MemberReply.Title}}{{else}}NONE{{/if}}</td></tr>
        {{/each}}
    </table>
    <div class="app"></div>
</section>