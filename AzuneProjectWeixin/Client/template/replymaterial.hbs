<header>
    <button class="button-back">Back</button>
    <h2 class="title">Reply Material</h2>
</header>
<section class="content">
    <table class="messages">
        <tr><th>Type</th><th>Title</th><th>Create Time</th><th>Content</th><th>Expand</th></tr>
        {{#each materials}}
        <tr id="{{idReplyMaterial}}"><td>{{MsgType}}</td><td>{{Title}}</td><td>{{CreateTime}}</td>{{#ifCond MsgType 'text'}}<td>{{Content}}</td><td></td>{{else}}{{#if Data.[0].Title}}<td>{{Data.[0].Title}}</td>{{else}}<td></td>{{/if}}<td><button class="expand">Expand</button></td>{{/ifCond}}</tr>        
        {{/each}}
    </table>
    <div class="app"></div>
</section>